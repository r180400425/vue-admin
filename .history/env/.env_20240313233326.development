# base url
# 定义了基础 URL，用于指定后端服务的地址和端口号。
VITE_BASE_URL = 'http://localhost:8089/server'
# VITE_BASE_URL = 'http://172.16.10.97:9091/server'

# file url
# 定义了文件 URL，通常用于指定文件服务的地址。
VITE_FILE_URL = 'your_file_url'

# api mock
# 设置为 true 表示启用 API 模拟，即在开发环境中模拟后端接口数据，方便前端开发调试。
VITE_API_MOCK = true

# menu mock
# 设置为 true 表示启用菜单模拟，即在开发环境中模拟菜单数据，用于前端页面展示和调试。
VITE_MENU_MOCK = true
